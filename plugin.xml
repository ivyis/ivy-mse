<?xml version="1.0" encoding="UTF-8"?>
<plugin title="ivymse" loader="DEFAULT">
<overlays>
		<overlay id="startup.ivymse" resourcebundle="content/ivymse/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="ivymse" label="${Launcher.ivymse}" command="mantleXulHandler.openUrl('${Launcher.ivymse}','${Launcher.ivymse_TOOLTIP}','plugin/ivymse/api/default')"/>
	      	</menubar>
	    </overlay>
	</overlays>
	<lifecycle-listener class="pt.webdetails.cpk.CpkLifecycleListener"/>
	<static-paths>
		<static-path url="/ivymse/static" localFolder="static"/>
		<static-path url="/ivymse/resources" localFolder="resources"/>
	</static-paths>
	<content-types>
		<content-type>
			<title>Ivymse</title>
			<description></description>
			<company name="" url="" logo="@PLUGIN_COMPANY_LOGO@"></company>
		</content-type>
	</content-types>
	<content-generator
		id="ivymse"
		title="Ivymse"
		type="ivymse"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="ivymse_main" anchor="tools-submenu" label="Ivymse" command="content/ivymse/" type="MENU_ITEM" how="LAST_CHILD"/>-->
	</menu-items>
</plugin>
